 <TEMPLATE MATCH="7..." Timeout="0"/>  <!-- Internal numbers -->
 <TEMPLATE MATCH="8..." Timeout="0"/>  <!-- Internal numbers -->
 <TEMPLATE MATCH="9..." Timeout="0"/>  <!-- Internal numbers -->
 <TEMPLATE MATCH="2.." Timeout="1"/> <!-- Internal Extensions -->
 <TEMPLATE MATCH="3.." Timeout="1"/>
